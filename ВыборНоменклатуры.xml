<?xml version="1.0" encoding="utf-8"?>

<unit name="뉌햠쮐햏쮏쨿왏쫧쥃햟혝혞혦">

	<type name="뉌햠쮐햏쮏쨿왏쫧쥃햟혝혞혦" scope="global">

		<state name="햆쮏쐃썛쒫쟴혧뉋뛣쮐향햦햫혞">
			<query>
				<insert into="쮏쥄햨햟">
					<put field="owner" in="햏쮏쨿왏쫧쥃햟혝혞햟"/> <!--as-->
				</insert>
			</query>
		</state>
		
		<state name="뉋뛣쮐향햦햫햣">
			<function name="title">
				<get value=" 햨쮐향햦햫햣"/>
			</function>
		</state>

		<function name="뉋뛣쮐향햦햫햣">
			<seek>
				<get value="쮏쥄햨햟"/>
				<account/>
				<get field="owner"/>
			</seek>
		</function>
		
		<access.allow role="햃햢햪햦햫햦혜혝햟혝쮐"/>
		<access.allow role="쮐햣혝햦혝햣햩혧"/>
	</type>

	<form name="뉌햠쮐햏쮏쨿왏쫧쥃햟혝혞혦">
		<row/>
		<row indent="1" text="햌혞햦혝혧"/>
		<cell>
			<button name="햆쮏쐃썛쒫쟴혧뉋뛣쮐향햦햫혞" text="햆쮏쐃썛쒫쟴혧  햨쮐향햦햫혞"/>
		</cell>

	</form>

	<form name="뉌햠쮐햏쮏쨿왏쫧쥃햟혝혞혦.뉋뛣쮐향햦햫햣">
		<row/>
		<row indent="1" text=" 햨쮐향햦햫햣"/>
		<cell bold="1" color="00C000">
			<link reference="햌쮐향햦햫햟" type="report" text="游 햣햣햧혝햦  햨쮐향햦햫혞"/>
		</cell>
	</form>
</unit>
